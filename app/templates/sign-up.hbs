<div class="row" style="background:#00BCD2;">
  <div class="col s12 m6 offset-m3">
    <h5 class="center-align" style="margin-top: 3%;">Sign Up</h5>
    <h6 class="center-align" style="margin-top: 3%;">RaffelIt</h6>
    <div class="center-align"><a class="waves-effect waves-light btn light-blue darken-4" style="width: 80%; margin-top: 3%;"><i class="fa fa-facebook fa-2x" style="margin-right: 3%;" aria-hidden="true"></i>Entrar con Facebook</a></div>
    <div class="center-align"><a class="waves-effect waves-light btn light red darken-4" style="width: 80%; margin-top: 3%; margin-bottom: 3%;"><i class="fa fa-google fa-2x" style="margin-right: 3%;" aria-hidden="true"></i>Entrar con Google +</a></div>
    <div class="divider"></div>
    <!--form-->
    <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">account_circle</i>
          {{input id="name" type="text" class="validate" value=model.nombre}}
          <label for="name">Nombre</label>
        </div>
        <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">account_circle</i>
          {{input id="last_name" type="text" class="validate c-w" value=model.apellidos}}
          <label for="last_name">Apellido</label>
        </div>
        <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">email</i>
          {{input id="email" type="email" class="validate c-w" value=model.email}}
          <label for="email">Correo Electronico</label>
        </div>
        <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">filter_tilt_shift</i>
          {{input id="age" type="number" class="validate c-w" value=model.edad}}
          <label for="age">Edad</label>
        </div>
        <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">vpn_key</i>
          {{input id="password" type="password" class="validate c-w" value=pass}}
          <label for="password">Contraseña</label>
        </div>
        {{!-- <div class="input-field col s12 m8 offset-m2">
          <i class="material-icons prefix">vpn_key</i>
          {{input id="val_password" type="password" class="validate" value=validarpass focus-out}}
          <label for="val_password">Validar contraseña</label>
        </div> --}}
      </div>
    </form>
    {{#link-to 'index' class="waves-effect waves-light btn left red"}}<i class="material-icons right">cancel</i>Cancelar{{/link-to}}
    <a {{action 'registrar' model}}class="waves-effect waves-light btn right blue"><i class="material-icons right">done</i>Registrar</a>
  </div>
  </div>
</div>